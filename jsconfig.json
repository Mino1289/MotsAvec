{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES3",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "@/modules/*": [
                "./src/store/modules/*"
            ]
        }
    },
    "exclude": [
        "node_modules",
        "dist"
    ],
    "include": [
        "src/**/*"
    ]
}